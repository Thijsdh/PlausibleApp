{
  "name": "plausibleapp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "test:e2e:ios:build": "detox build --configuration ios.sim.release",
    "test:e2e:ios:run": "detox test --configuration ios.sim.release",
    "test:e2e:ios": "npm run test:e2e:ios:build && start-server-and-test start-mock-server http://localhost:3000 test:e2e:ios:run",
    "test:e2e:android:build": "detox build --configuration android.emu.release",
    "test:e2e:android:run": "detox test --configuration android.emu.release",
    "test:e2e:android:run-with-server": "start-server-and-test start-mock-server http://localhost:3000 test:e2e:android:run",
    "test:e2e:android": "npm run test:e2e:android:build && npm run test:e2e:android:run-with-server",
    "start-mock-server": "ts-node ./mock-server/index.ts",
    "take-screenshots": "detox test --config-path ./.detoxrc.screenshots.js --configuration android screenshots/screenshots.spec.ts",
    "screenshots": "npm run test:e2e:android:build && start-server-and-test start-mock-server http://localhost:3000 take-screenshots",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/react-native-fontawesome": "^0.3.0",
    "@react-native-async-storage/async-storage": "^1.18.1",
    "@react-native-cookies/cookies": "^6.1.0",
    "@react-native-picker/picker": "^2.4.10",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/native-stack": "^6.9.12",
    "country-list": "^2.2.0",
    "dayjs": "^1.11.7",
    "polished": "^4.2.2",
    "react": "18.2.0",
    "react-native": "0.71.7",
    "react-native-linear-gradient": "^2.6.2",
    "react-native-safe-area-context": "^4.5.2",
    "react-native-screens": "^3.20.0",
    "react-native-svg": "^13.9.0",
    "swr": "^2.1.5",
    "victory-native": "^36.6.10"
  },
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/preset-env": "^7.21.5",
    "@babel/runtime": "^7.21.5",
    "@react-native-community/eslint-config": "^3.2.0",
    "@semantic-release/git": "^10.0.1",
    "@tsconfig/react-native": "^3.0.1",
    "@types/country-list": "^2.1.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.1",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "babel-jest": "^29.2.1",
    "detox": "^20.8.0",
    "eslint": "^8.40.0",
    "express": "^4.18.2",
    "jest": "^29.5.0",
    "metro-react-native-babel-preset": "^0.76.3",
    "react-test-renderer": "18.2.0",
    "semantic-release": "^21.0.2",
    "start-server-and-test": "^2.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
